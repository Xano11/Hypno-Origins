                },
                {
                    "type": "origins:selector_action",
                    "selector": "@n[tag=bind_domain,type=bee]",
                    "bientity_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:add_to_entity_set",
