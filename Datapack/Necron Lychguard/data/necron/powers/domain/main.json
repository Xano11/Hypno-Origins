                        "bientity_action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:execute_command",
                                        "command": "function necron:domain/shoot_wave_domain"
                                    }
                                },
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:delay",
                                        "ticks": 4,
                                        "action": {
                                            "type": "origins:area_of_effect",
                                            "radius": 16,
                                            "shape": "sphere",
                                            "bientity_action": {
                                                "type": "origins:target_action",
                                                "action": {
                                                    "type": "origins:execute_command",
                                                    "command": "function necron:domain/dampened_domain"
                                                }
                                            },
                                            "bientity_condition": {
                                                "type": "origins:and",
                                                "conditions": [
                                                    {
                                                        "type": "origins:target_condition",
                                                        "condition": {
                                                            "type": "origins:living"
                                                        }
                                                    },
                                                    {
                                                        "type": "origins:invert",
                                                        "condition": {
                                                            "type": "origins:in_entity_set",
                                                            "set": "necron:domain/main_binded"
                                                        }
                                                    }
                                                ]
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "else_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "function necron:domain/shoot_wave_player"
                            },
                            {
                                "type": "origins:delay",
                                "ticks": 4,
                                "action": {
                                    "type": "origins:area_of_effect",
                                    "radius": 16,
                                    "shape": "sphere",
                                    "bientity_action": {
                                        "type": "origins:target_action",
                                        "action": {
                                            "type": "origins:execute_command",
                                            "command": "function necron:domain/dampened_player"
                                        }
                                    }
                                }
                            }
                        ]
                    }
                },
                {
                    "type": "origins:selector_action",
                    "selector": "@n[tag=bind_domain,type=bee]",
                    "bientity_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:add_to_entity_set",
                                "set": "necron:domain/main_binded"
                            },
                            {
                                "type": "origins:target_action",
                                "action": {
                                    "type": "origins:execute_command",
                                    "command": "tag @s remove bind_domain"
                                }
                            }
