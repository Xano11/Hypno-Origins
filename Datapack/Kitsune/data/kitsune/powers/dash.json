    "roll": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:exhaust",
                    "amount": 1
                },
                {
                    "type": "origins:change_resource",
                    "resource": "kitsune:dash_timer",
                    "change": 2,
                    "operation": "set"
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.horse.saddle player @a ~ ~ ~ 1 2"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "kitsune:dash_cooldown",
                    "change": 20,
                    "operation": "set"
                },
                {
                    "type": "origins:add_velocity",
                    "x": 0,
                    "z": 0,
                    "set": true,
                    "space": "local_horizontal_normalized"
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "kitsune:dash_forward_held",
                        "comparison": ">",
                        "compare_to": 0
                    },
                    "if_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:add_velocity",
                                "z": 1,
                                "space": "local_horizontal_normalized"
                            },
                            {
                                "type": "origins:delay",
                                "ticks": 2,
                                "action": {
                                    "type": "origins:add_velocity",
                                    "space": "local_horizontal_normalized",
                                    "set": true,
                                    "z": 0.4
                                }
                            }
                        ]
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "kitsune:dash_back_held",
                        "comparison": ">",
                        "compare_to": 0
                    },
                    "if_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:add_velocity",
                                "z": -1,
                                "space": "local_horizontal_normalized"
                            },
                            {
                                "type": "origins:delay",
                                "ticks": 2,
                                "action": {
                                    "type": "origins:add_velocity",
                                    "space": "local_horizontal_normalized",
                                    "set": true,
                                    "z": -0.4
                                }
                            }
                        ]
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "kitsune:dash_left_held",
                        "comparison": ">",
                        "compare_to": 0
                    },
                    "if_action": {
                        "type": "origins:and",
