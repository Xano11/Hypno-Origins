{
    "type": "origins:multiple",
    "target": {
        "type": "origins:entity_set"
    },
    "falling": {
        "type": "origins:modify_falling",
        "velocity": 0.0001,
        "take_fall_damage": false
    },
    "duration": {
        "type": "origins:resource",
        "min": 0,
        "max": 120,
        "start_value": 120,
        "min_action": {
            "type": "origins:execute_command",
            "command": "kill @s"
        }
    },
    "duration_tick": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "chosen:spells/homing_orb_duration",
                    "change": -1
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:soul_fire_flame ~ ~1 ~ 0 0 0 0 1 normal"
                }
            ]
        },
        "interval": 1
    },
    "collision": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:action_on_entity_set",
            "set": "chosen:spells/homing_orb_target",
            "bientity_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:damage",
                        "amount": 2,
                        "damage_type": "chosen:sorcery"
                    },
                    {
                        "type": "origins:target_action",
                        "action": {
                            "type": "origins:execute_command",
                            "command": "function chosen:homing_hit"
                        }
                    },
                    {
                        "type": "origins:actor_action",
                        "action": {
                            "type": "origins:execute_command",
                            "command": "kill @s"
                        }
                    }
                ]
            },
            "bientity_condition": {
                "type": "origins:distance",
                "comparison": "<=",
                "compare_to": 1
            }
        },
        "interval": 1
    },
    "moving": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:action_on_entity_set",
            "set": "chosen:spells/homing_orb_target",
            "bientity_action": {
                "type": "origins:invert",
                "action": {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:target_condition",
                        "condition": {
                            "type": "origins:resource",
                            "resource": "chosen:spells/homing_orb_duration",
                            "comparison": "<",
                            "compare_to": 60
                        }
                    },
                    "if_action": {
                        "type": "origins:add_velocity",
                        "z": -0.6,
                        "y": 0,
                        "set": true
                    },
                    "else_action": {
                        "type": "origins:add_velocity",
                        "z": -0.4,
                        "y": 0,
                        "set": true
                    }
                }
            }
        },
        "interval": 1,
        "condition": {
            "type": "origins:resource",
            "resource": "chosen:spells/homing_orb_duration",
            "comparison": "<",
            "compare_to": 105
        }
    },
    "hidden": true
}