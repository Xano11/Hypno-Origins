                    "operation": "set"
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.breeze.land player @a ~ ~ ~ 1 0.85"
                },
                {
                    "type": "origins:if_else_list",
                    "actions": [
                        {
                            "condition": {
                                "type": "origins:attribute",
                                "attribute": "minecraft:generic.armor",
                                "comparison": ">=",
                                "compare_to": 16.0
                            },
                            "action": {
                                "type": "origins:change_resource",
                                "resource": "chosen:roll_heavy_cooldown",
                                "change": 100,
                                "operation": "set"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:and",
                                "conditions": [
                                    {
                                        "type": "origins:attribute",
                                        "attribute": "minecraft:generic.armor",
                                        "comparison": "<",
                                        "compare_to": 16.0
                                    },
                                    {
                                        "type": "origins:attribute",
                                        "attribute": "minecraft:generic.armor",
                                        "comparison": ">",
                                        "compare_to": 10.0
                                    }
                                ]
                            },
                            "action": {
                                "type": "origins:change_resource",
                                "resource": "chosen:roll_medium_cooldown",
                                "change": 70,
                                "operation": "set"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:attribute",
                                "attribute": "minecraft:generic.armor",
                                "comparison": "<=",
                                "compare_to": 10.0
                            },
                            "action": {
                                "type": "origins:change_resource",
                                "resource": "chosen:roll_light_cooldown",
                                "change": 40,
                                "operation": "set"
                            }
                        }
                    ]
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "chosen:roll_forward_held",
                        "comparison": ">",
                        "compare_to": 0
                    },
                    "if_action": {
                        "type": "origins:add_velocity",
                        "z": 0.85,
                        "space": "local_horizontal_normalized"
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "chosen:roll_back_held",
                        "comparison": ">",
                        "compare_to": 0
                    },
                    "if_action": {
                        "type": "origins:add_velocity",
                        "z": -0.85,
                        "space": "local_horizontal_normalized"
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "chosen:roll_left_held",
                        "comparison": ">",
                        "compare_to": 0
                    },
                    "if_action": {
                        "type": "origins:add_velocity",
                        "x": 0.85,
                        "space": "local_horizontal_normalized"
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "chosen:roll_right_held",
                        "comparison": ">",
                        "compare_to": 0
                    },
                    "if_action": {
                        "type": "origins:add_velocity",
                        "x": -0.85,
                        "space": "local_horizontal_normalized"
                    }
