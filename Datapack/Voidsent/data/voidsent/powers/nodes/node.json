{
    "type": "origins:multiple",
    "spin": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0
    },
    "particles": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "particle minecraft:witch ~ ~0.2 ~ 0.1 0.1 0.1 0 4"
        },
        "interval": 1
    },
    "vanish": {
        "type": "origins:resource",
        "min": 0,
        "max": 30,
        "start_value": 30,
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "function voidsent:abilities/tp_particles"
                },
                {
                    "type": "origins:execute_command",
                    "command": "function voidsent:abilities/remove_node"
                }
            ]
        }
    },
    "detect_vanish": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:selector_action",
            "selector": "@a",
            "bientity_action": {
                "type": "origins:actor_action",
                "action":  {
                    "type": "origins:change_resource",
                    "resource": "voidsent:nodes/node_vanish",
                    "change": 30,
                    "operation": "set"
                }
            },
            "bientity_condition": {
                "type": "origins:invert",
                "condition": {
                    "type": "origins:in_entity_set",
                    "set": "voidsent:nodes/ability_nodes"
                }
            }
        },
        "interval": 1
    },
    "no_owner": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "voidsent:nodes/node_vanish",
            "change": -1
        },
        "interval": 1
    },
    "load": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:command",
                "command": "forceload query ~ ~",
                "comparison": "==",
                "compare_to": 1
            },
            "if_action": {
                "type": "origins:execute_command",
                "command": "power revoke @s voidsent:nodes/node_load"
            },
            "else_action": {
                "type": "origins:execute_command",
                "command": "forceload add ~ ~ ~ ~"
            }
        },
        "execute_chosen_when_orb": false
    },
    "damage": {
        "type": "origins:resource",
        "min": 0,
        "max": 2,
        "start_value": 0,
        "max_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "function voidsent:abilities/tp_particles"
                },
                {
                    "type": "origins:execute_command",
                    "command": "function voidsent:abilities/remove_node"
                }
            ]
        }
    },
    "break": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:execute_command",
            "command": "execute on passengers if entity @s[tag=node_interaction] run function voidsent:abilities/interaction_hit"
        },
        "interval": 1,
        "condition": {
            "type": "origins:passenger",
            "bientity_condition": {
                "type": "origins:actor_condition",
                "condition": {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:entity_type",
                            "entity_type": "minecraft:interaction"
                        },
                        {
                            "type": "origins:command",
                            "command": "execute if data entity @s attack",
                            "comparison": "==",
                            "compare_to": 1
                        }
                    ]
                }
            }
        }
    },
    "hidden": true
}