{
    "type": "origins:multiple",
    "windup": {
        "type": "origins:resource",
        "min": 0,
        "max": 60,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "resource": "voidsent:dimension/gate_interrupt",
                "comparison": "==",
                "compare_to": 0
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:execute_command",
                        "command": "effect clear @s minecraft:darkness"
                    },
                    {
                        "type": "origins:spawn_entity",
                        "entity_type": "minecraft:armor_stand",
                        "tag": "{Silent:1b,Invulnerable:1b,Marker:1b,Invisible:1b,Tags:[\"voidsent_portal\"]}",
                        "entity_action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:execute_command",
                                    "command": "power grant @s voidsent:dimension/portal"
                                },
                                {
                                    "type": "origins:execute_command",
                                    "command": "function voidsent:abilities/spawn_portal"
                                }
                            ]
                        }
                    }
                ]
            }
        }
    },
    "windup_up": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "voidsent:dimension/gate_windup",
            "change": -1
        },
        "interval": 1,
        "condition": {
            "type": "origins:resource",
            "resource": "voidsent:dimension/gate_windup",
            "comparison": ">",
            "compare_to": 0
        }
    },
    "enter": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "voidsent:dimension/gate_windup",
                    "change": 60,
                    "operation": "set"
                },
                {
                    "type": "origins:execute_command",
                    "command": "effect give @s minecraft:darkness 3 0 false"
                }
            ]
        },
        "cooldown": 1,
        "key": "key.origins.secondary_active",
        "hud_render": {
            "should_render": false
        },
        "condition": {
            "type": "origins:dimension",
            "dimension": "voidsent:void",
            "inverted": true
        }
    },
    "interrupt": {
        "type": "origins:resource",
        "min": 0,
        "max": 20,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        }
    },
    "blinking": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "function voidsent:abilities/opening_gate"
        },
        "interval": 15,
        "condition": {
            "type": "origins:resource",
            "resource": "voidsent:dimension/gate_windup",
            "comparison": ">",
            "compare_to": 0
        }
    },
    "interrupt_ticking": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "voidsent:dimension/gate_interrupt",
            "change": -1
        },
        "interval": 1,
        "condition": {
            "type": "origins:resource",
            "resource": "voidsent:dimension/gate_interrupt",
            "comparison": ">",
            "compare_to": 0
        }
    },
    "damaged": {
        "type": "origins:action_when_damage_taken",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "voidsent:dimension/gate_interrupt",
                    "change": 20,
                    "operation": "set"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "voidsent:dimension/gate_windup",
                    "change": 0,
                    "operation": "set"
                }
            ]
        },
        "cooldown": 1
    },
    "name": "Inner: Control",
    "description": "Entering a state of total focus is imperative for great feats.",
    "badges": [
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/community/silent/badge/shapes/triangle_up.png",
            "text": "§e§lInner: Control §r§e👁§r\n§e§lCONTROL§7 is the energy required for §lUnleash: Control§r§7 and takes §e2m§7 to passively fully charge.\n\nWhilst holding the ability key, you charge §a12x§7 time quicker.\nWhilst actively charging, you §ccannot§7 move and you close your eyes; making you §cfully blind§7 and incapable of using items.\n\nIf §e§lCONTROL§7 is at max, you will be able to see all living entities glow within §e32§7 blocks.\n\n§7§lActive Ability"
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/toggle.png",
            "text": [
                "§7Hold §r[",
                {
                    "keybind": "key.origins.secondary_active"
                },
                "]§7 to enter a focusing state."
            ]
        }
    ]
}