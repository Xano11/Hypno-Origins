{
    "type": "origins:multiple",
    "cat": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:area_of_effect",
                    "radius": 4,
                    "bientity_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:damage",
                                "amount": 1,
                                "damage_type": "clown:deception"
                            },
                            {
                                "type": "origins:add_velocity",
                                "x": 0,
                                "y": 0,
                                "z": -0.025,
                                "client": true,
                                "server": true,
                                "set": true
                            },
                            {
                                "type": "origins:target_action",
                                "action": {
                                    "type": "origins:execute_command",
                                    "command": "function cleavemaster:vfx/slice"
                                }
                            }
                        ]
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "tp @s ^ ^ ^2"
                },
                {
                    "type": "origins:execute_command",
                    "command": "fill ~-2 ~ ~-2 ~2 ~ ~2 air destroy"
                },
                {
                    "type": "origins:execute_command",
                    "command": "execute positioned as @e[tag=cleavemaster.sawblade2] as @e[tag=cleavemaster.sawblade2] at @e[tag=cleavemaster.sawblade2] run tp @e[tag=cleavemaster.sawblade2] ~ ~ ~ ~12 ~"
                }
            ]
        }
    },
    "die": {
        "type": "origins:action_over_time",
        "interval": 200,
        "entity_action": {
            "type": "origins:execute_command",
            "command": "kill @e[tag=cleavemaster.sawblade]"
        }
    }
}