    },
    "block_resonance_set": {
        "type": "origins:entity_set"
    },
    "nails": {
        "type": "origins:resource",
        "min": 0,
        "max": 9,
        "start_value": 9,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png",
            "bar_index": 15
        }
    },
    "delay": {
        "type": "origins:resource",
        "min": 0,
        "max": 50,
        "start_value": 50
    },
    "delay_tick": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "resonant:resonance/main_delay",
            "change": 1
        },
        "interval": 1
    },
    "recharge": {
        "type": "origins:resource",
        "min": 0,
        "max": 30,
        "start_value": 0,
        "max_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "resonant:resonance/main_nails",
                    "change": 1
                },
                {
                    "type": "origins:change_resource",
                    "resource": "resonant:resonance/main_recharge",
                    "change": 0,
                    "operation": "set"
                }
            ]
        }
    },
    "recharge_tick": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "resonant:resonance/main_recharge",
            "change": 1
        },
        "interval": 1,
        "condition": {
            "type": "origins:resource",
            "resource": "resonant:resonance/main_delay",
            "comparison": "==",
            "compare_to": 50
        }
    },
    "double_check": {
        "type": "origins:resource",
        "min": 0,
        "max": 5,
        "start_value": 0,
        "max_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:resource",
                "resource": "resonant:resonance/main_nails",
                "comparison": ">=",
                "compare_to": 3
            },
            "if_action": {
                "type": "origins:execute_command",
                "command": "execute anchored eyes positioned ^ ^-0.3 ^1.5 rotated as @s run function resonant:summon_nails"
            }
        },
        "min_action": {
            "type": "origins:execute_command",
            "command": "execute if entity @s[tag=!resonant_double] anchored eyes positioned ^ ^-0.3 ^1.5 rotated as @s run function resonant:summon_nail"
        },
        "hud_render": {
            "should_render": false
        }
    },
    "double_check_decay": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "resonant:resonance/main_double_check",
            "change": -1
        },
        "interval": 1
    },
    "create_nail": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "resonant:resonance/main_double_check",
            "change": 4
        },
        "cooldown": 1,
        "key": "key.origins.primary_active",
        "hud_render": {
            "should_render": false
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:command",
                    "command": "execute anchored eyes positioned ^ ^-0.3 ^1.5 unless entity @e[tag=resonant_nail,distance=..0.1]",
                    "comparison": ">=",
                    "compare_to": 1
                },
                {
                    "type": "origins:resource",
                    "resource": "resonant:resonance/main_nails",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "inverted": true,
                    "type": "origins:raycast",
                    "distance": 1.6,
                    "block": true,
                    "entity": false,
                    "shape_type": "collider",
                    "fluid_handling": "none",
                    "hit_bientity_condition": {
                        "type": "origins:target_condition",
                        "condition": {
                            "type": "origins:living"
                        }
                    }
                }
            ]
        }
    },
    "hit_nail": {
        "type": "origins:action_on_hit",
        "bientity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:has_command_tag",
                    "command_tag": "main_resonant_nail"
                }
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:actor_action",
                        "action": {
                            "type": "origins:execute_command",
                            "command": "tag @s add nail_hit"
                        }
                    },
                    {
                        "type": "origins:target_action",
                        "action": {
                            "type": "origins:riding_action",
                            "action": {
                                "type": "origins:execute_command",
                                "command": "function resonant:hit_nails"
                            }
                        }
                    },
                    {
                        "type": "origins:actor_action",
                        "action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:execute_command",
                                    "command": "tag @s remove nail_hit"
                                },
                                {
                                    "type": "origins:selector_action",
                                    "selector": "@e[tag=nail_set,type=#resonant:set_add]",
                                    "bientity_action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:add_to_entity_set",
                                                "set": "resonant:resonance/main_nail_set"
                                            },
                                            {
                                                "type": "origins:target_action",
                                                "action": {
                                                    "type": "origins:execute_command",
                                                    "command": "tag @s remove nail_set"
                                                }
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    }
                ]
            },
            "else_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:actor_action",
                        "action": {
                            "type": "origins:execute_command",
                            "command": "tag @s add nail_hit"
                        }
                    },
                    {
                        "type": "origins:target_action",
                        "action": {
                            "type": "origins:riding_action",
                            "action": {
                                "type": "origins:execute_command",
                                "command": "function resonant:hit_nail"
                            }
                        }
                    },
                    {
                        "type": "origins:actor_action",
                        "action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:execute_command",
                                    "command": "tag @s remove nail_hit"
                                },
                                {
                                    "type": "origins:selector_action",
                                    "selector": "@e[tag=nail_set,type=#resonant:set_add]",
                                    "bientity_action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:add_to_entity_set",
                                                "set": "resonant:resonance/main_nail_set"
                                            },
                                            {
                                                "type": "origins:target_action",
                                                "action": {
                                                    "type": "origins:execute_command",
                                                    "command": "tag @s remove nail_set"
                                                }
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    }
                ]
            }
        },
        "bientity_condition": {
            "type": "origins:target_condition",
            "condition": {
                "type": "origins:has_command_tag",
                "command_tag": "resonant_nail"
            }
        }
    },
    "trigger_pins": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:selector_action",
                    "selector": "@e[type=armor_stand,tag=resonant_nail]",
                    "bientity_action": {
                        "type": "origins:target_action",
                        "action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:block_action_at",
                                    "block_action": {
                                        "type": "origins:area_of_effect",
                                        "radius": 2,
                                        "shape": "star",
                                        "block_action": {
                                            "type": "origins:execute_command",
                                            "command": "execute unless entity @e[type=marker,tag=resonance_block,dx=0] unless block ~ ~ ~ #replaceable run function resonant:block_resonance"
                                        },
                                        "block_condition": {
                                            "type": "origins:and",
                                            "conditions": [
                                                {
                                                    "type": "origins:movement_blocking"
                                                },
                                                {
                                                    "type": "origins:block_state",
                                                    "property": "facing",
                                                    "inverted": true
                                                }
                                            ]
                                        }
                                    }
                                },
                                {
                                    "type": "origins:execute_command",
                                    "command": "function resonant:nail_explode"
                                },
                                {
                                    "type": "origins:area_of_effect",
                                    "radius": 2.5,
                                    "shape": "sphere",
                                    "bientity_action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:add_velocity",
                                                "z": 0.8,
                                                "y": 0.05
                                            },
                                            {
                                                "type": "origins:target_action",
                                                "action": {
                                                    "type": "origins:and",
                                                    "actions": [
                                                        {
                                                            "type": "origins:execute_command",
                                                            "command": "tag @s add nail_target"
                                                        },
                                                        {
                                                            "type": "origins:set_fall_distance",
                                                            "fall_distance": 0
                                                        }
                                                    ]
                                                }
                                            }
                                        ]
                                    },
                                    "bientity_condition": {
                                        "type": "origins:target_condition",
                                        "condition": {
                                            "type": "origins:living"
                                        }
                                    }
                                },
                                {
                                    "type": "origins:execute_command",
                                    "command": "function resonant:kill_nail"
                                }
                            ]
                        }
                    },
                    "bientity_condition": {
                        "type": "origins:in_entity_set",
                        "set": "resonant:resonance/main_nail_set"
                    }
                },
                {
                    "type": "origins:selector_action",
                    "selector": "@e[tag=nail_target]",
                    "bientity_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:if_else",
                                "condition": {
                                    "type": "origins:equal",
                                    "inverted": true
                                },
                                "if_action": {
                                    "type": "origins:and",
                                    "actions": [
                                        {
                                            "type": "origins:damage",
                                            "amount": 4,
                                            "damage_type": "resonant:nail"
                                        },
                                        {
                                            "type": "origins:damage",
                                            "amount": 2,
                                            "damage_type": "resonant:nail_true"
                                        }
                                    ]
                                }
                            },
                            {
                                "type": "origins:target_action",
                                "action": {
                                    "type": "origins:execute_command",
                                    "command": "tag @s remove nail_target"
                                }
                            }
                        ]
                    }
                },
                {
                    "type": "origins:selector_action",
                    "selector": "@e[type=item_display,tag=resonance_block]",
                    "bientity_action": {
                        "type": "origins:add_to_entity_set",
                        "set": "resonant:resonance/main_block_resonance_set"
                    }
                }
            ]
        },
        "cooldown": 1,
        "key": "key.origins.secondary_active",
        "hud_render": {
            "should_render": false
        },
        "condition": {
            "type": "origins:entity_set_size",
            "set": "resonant:resonance/main_nail_set",
            "comparison": ">",
            "compare_to": 0
        }
    },
